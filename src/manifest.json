{
	"manifest_version": 2,
	"name": "Better Misskey",
	"description": "misskey.xyz で機能を補います。",
	"version": "1.4.9",
	"homepage_url": "https://github.com/S4WA/Better-Misskey",
	"background": {
		"scripts": [
			"bg.js",
			"utils.js"
		]
	},
	"content_scripts": [ {
		"all_frames": true,
		"js": [
			"misskey.js"
		],
		"matches": [ "*://misskey.xyz/*", "*://misskey.io/*" ],
		"run_at": "document_end"
	} ],
	"permissions": [
		"tabs", "contextMenus", "storage", "*://misskey.xyz/*", "*://misskey.io/*"
	],
	"options_ui": {
		"page": "options/options.html",
		"chrome_style": true,
		"open_in_tab": true
	},
	"browser_action": {
		"default_icon": "files/icon.png",
		"default_popup": "options/popup.html"
	},
	"icons": {
		"16": "files/icon.png",
		"48": "files/icon.png",
		"128": "files/icon.png"
	}
}